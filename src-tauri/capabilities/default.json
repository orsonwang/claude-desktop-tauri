{
  "$schema": "https://schema.tauri.app/config/2/capability",
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "remote": {
    "urls": ["https://claude.ai/*"]
  },
  "permissions": [
    "core:default",
    "shell:allow-open",
    "shell:default",
    "dialog:allow-open"
  ]
}
